{
  "name": "@junctionrelay/plugin-generic-api",
  "version": "1.0.0",
  "description": "Generic API collector plugin for JunctionRelay",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "junctionrelay": {
    "type": "collector",
    "entry": "dist/index.js"
  },
  "scripts": {
    "build": "esbuild src/index.ts --bundle --platform=node --format=esm --outfile=dist/index.js",
    "pack": "rm -rf out && mkdir -p out/generic-api/dist && cp package.json out/generic-api/ && cp dist/index.js out/generic-api/dist/ && cd out && zip -r ../generic-api.zip generic-api && cd .. && rm -rf out",
    "clean": "rm -rf dist out"
  },
  "dependencies": {
    "@junctionrelay/collector-sdk": "1.0.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/catapultcase/JunctionRelay_Collectors.git",
    "directory": "plugins/generic-api"
  }
}
