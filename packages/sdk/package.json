{
  "name": "@junctionrelay/collector-sdk",
  "version": "1.0.0",
  "description": "Plugin SDK for building JunctionRelay collector plugins",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "bin": {
    "rpc-host": "./bin/rpc-host.mjs"
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "npx tsx --test src/__tests__/*.test.ts",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@junctionrelay/collector-protocol": "1.0.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/catapultcase/JunctionRelay_Collectors.git",
    "directory": "packages/sdk"
  }
}
